{
    "title": "Audio to VTT subtitle",
    "description": "Convert audio files to VTT subtitles using Fast Whisper",
    "type": "serverless",
    "category": "audio",
    "iconUrl": "https://example.com/icon.png",
    "config": {
      "runsOn": "GPU",
      "containerDiskInGb": 10,
      "env": [
        {
          "key": "DEBUG",
          "value": "False"
        },
        {
          "key": "WHISPER_MODEL",
          "value": "large-v3"
        },
        {
          "key": "WHISPER_DEVICE",
          "value": "cuda"
        },
        {
          "key": "WHISPER_COMPUTE_TYPE",
          "value": "float16"
        },
        {
          "key": "WHISPER_BEAM_SIZE",
          "value": "5"
        }
      ]
    }
  }